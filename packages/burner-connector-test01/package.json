  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
